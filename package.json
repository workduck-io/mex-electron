{
  "name": "mex",
  "productName": "mex",
  "version": "1.0.0",
  "description": "mexMEXmex",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "xypnox",
    "email": "xypnox@workduck.io"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "mex"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "background": "./assets/icon.png",
            "format": "ULFO"
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/index.tsx",
                  "name": "mex_window"
                },
                {
                  "html": "./src/Spotlight/index.html",
                  "js": "./src/Spotlight/index.tsx",
                  "name": "spotlight_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.59",
    "@electron-forge/maker-deb": "^6.0.0-beta.59",
    "@electron-forge/maker-dmg": "^6.0.0-beta.59",
    "@electron-forge/maker-rpm": "^6.0.0-beta.59",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.59",
    "@electron-forge/maker-zip": "^6.0.0-beta.59",
    "@electron-forge/plugin-webpack": "6.0.0-beta.59",
    "@iconify-icons/bx": "^1.1.0",
    "@iconify-icons/codicon": "^1.1.15",
    "@iconify-icons/gg": "^1.1.0",
    "@iconify-icons/majesticons": "^1.1.3",
    "@iconify-icons/ph": "^1.1.3",
    "@iconify-icons/radix-icons": "^1.1.2",
    "@iconify-icons/ri": "^1.1.0",
    "@iconify/icons-simple-icons": "^1.1.36",
    "@iconify/react": "^3.0.1",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@vercel/webpack-asset-relocator-loader": "1.6.0",
    "css-loader": "^6.0.0",
    "electron": "13.1.9",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@testing-library/react": "^12.0.0",
    "@udecode/plate": "^1.1.8",
    "@uifabric/styling": "^7.19.0",
    "active-win": "^7.6.0",
    "axios": "^0.21.1",
    "caniuse-lite": "^1.0.30001228",
    "chokidar": "^3.5.2",
    "downshift": "^6.1.3",
    "electron-squirrel-startup": "^1.0.0",
    "fast-deep-equal": "^3.1.3",
    "history": "^5.0.1",
    "hotkeys-js": "^3.8.7",
    "nanoid": "^3.1.23",
    "polished": "^4.1.2",
    "ramda": "^0.27.1",
    "rc-tree": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.10.1",
    "react-hot-toast": "^2.1.0",
    "react-linkify": "^1.0.0-alpha",
    "react-modal": "^3.14.2",
    "react-oembed-container": "^1.0.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.1",
    "react-sortablejs": "^6.0.0",
    "react-tooltip": "^4.2.19",
    "react-vis-network-graph": "^3.0.1",
    "react-window": "^1.8.6",
    "remark-parse": "^9.0.0",
    "remark-slate": "^1.8.0",
    "robotjs": "^0.6.0",
    "sanitize-html": "^2.4.0",
    "slate": "^0.65.3",
    "slate-history": "^0.62.0",
    "slate-hyperscript": "^0.62.0",
    "slate-react": "^0.65.2",
    "sortablejs": "^1.13.0",
    "styled-components": "^5.3.0",
    "tinykeys": "^1.2.0",
    "twin.macro": "^2.6.2",
    "unified": "^9.2.1",
    "use-debounce": "^7.0.0",
    "zustand": "^3.5.8"
  }
}
